{
  "name": "@neutronpay/wallet-gateway",
  "version": "0.0.1",
  "main": "dist/index",
  "typings": "dist/index",
  "description": "Awesome project developed with TypeORM.",
  "graphql-doctor": {
    "schema.public.graphql": {
      "ref": "heads/master",
      "schemaPath": "./src/schema.graphql"
    }
  },
  "scripts": {
    "gen-gql": "rimraf ./graphql & gqlg --schemaFilePath ./schema.graphql --destDirPath ./graphql",
    "start": " NODE_ENV=development& ts-node index.ts",
    "dev": " NODE_ENV=development& ts-node-dev index.ts",
    "build": "rimraf ./dist & tsc",
    "dockerize": "docker build -t account .",
    "start:dev": "concurrently \" npx cross-env redis-server \" \" NODE_ENV=development&  ts-node-dev --respawn index.ts --no-autorestart \" "
  },
  "resolutions": {
    "graphql": "^15.0.0",
    "**/graphql": "^15.0.0"
  },
  "dependencies": {
    "@apollo/federation": "^0.33.1",
    "@apollo/gateway": "^0.42.1",
    "apollo-server-cache-memcached": "^3.2.0",
    "apollo-server-express": "^3.4.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "fs": "0.0.1-security",
    "graphql": "^15.6.1",
    "graphql-redis-subscriptions": "^2.4.0",
    "graphql-tools": "^7.0.5",
    "ioredis": "^4.27.11",
    "jsonwebtoken": "^8.5.1",
    "neutronpay-wallet-common": "^1.0.1",
    "prom-client": "^14.0.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express-jwt": "^6.0.2",
    "@types/jsonwebtoken": "^8.5.5"
  }
}
