    "@types/uuid": "^8.3.0",
    "@types/winston": "^2.4.4",
    "@types/ws": "^7.4.7",
    "amqplib": "^0.8.0",
    "apollo-server": "^3.1.2",
    "apollo-server-cache-memcached": "^3.0.3",
    "apollo-server-express": "^3.1.2",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
