    "yup": "^0.32.8"
  },
  "graphql-doctor": {
    "schema.public.graphql": {
      "ref": "heads/master",
      "schemaPath": "./src/schema.graphql"
    }
  },
  "scripts": {
    "start": "pm2 start dev-eco.config.js",
    "start-dev": "concurrently \"redis-server \" \" NODE_ENV=development&  ts-node-dev --respawn src/index.ts --no-autorestart \" ",
    "start-dev-account": "NODE_ENV=development&  ts-node-dev --respawn src/modules/account-module/index.ts --no-autorestart",
    "start-dev-transfer": "NODE_ENV=development&  ts-node-dev --respawn src/modules/transfer-module/index.ts --no-autorestart",
    "start-dev-lnd": "NODE_ENV=development&  ts-node-dev --respawn src/modules/lnd-module/index.ts --no-autorestart",
    "start-dev-bank": "NODE_ENV=development&  ts-node-dev --respawn src/modules/bank-module/index.ts --no-autorestart",
    "start-services": "concurrently \"redis-server \" \"npm:start-dev-* \"",
    "start-gateway": "NODE_ENV=development&  ts-node-dev --respawn src/index.ts --no-autorestart",
    "dockerize-account": "docker build -f dockerfiles/account.dockerfile -t account .",
    "dockerize-bank": "docker build -f dockerfiles/bank.dockerfile -t bank .",
    "dockerize-transfer": "docker build -f dockerfiles/transfer.dockerfile -t transfer .",
    "dockerize-lnd": "docker build -f dockerfiles/lnd.dockerfile -t lnd .",
    "dockerize-gateway": "docker build -f dockerfiles/gateway.dockerfile -t gateway .",
    "dev": "concurrently \"redis-server \" \" NODE_ENV=development& pm2 start dev-eco.config.js --no-autorestart \" ",
    "dev:test": "concurrently \"redis-server \" \" SET NODE_ENV=test& ts-node-dev --respawn src/index.ts \"",
    "restart": "pm2 restart src/index.ts -i 3 --watch",
    "stop": "pm2 stop all",
    "test": "jest --watch --runInBand",
    "build": "rimraf ./dist & tsc",
    "watch": "tsc -w",
    "monit": "pm2 monit",
    "gen-gql": "rimraf ./modules/graphql & gqlg --schemaFilePath ./src/schema.graphql --destDirPath ./src/modules/graphql --depthLimit 5",
    "gen-env": "npx gen-env-types .env -o src/types/env.d.ts -e ."
  },
  "resolutions": {
    "graphql": "^15.0.0"
  }
}
