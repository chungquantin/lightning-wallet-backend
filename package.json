    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "express-session": "^1.17.1",
    "express-winston": "^4.1.0",
    "faker": "^5.4.0",
    "fetch-cookie": "^0.11.0",
